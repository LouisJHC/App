    <%= form_for @post do |f| %>
        <p>
            <%= f.label :title %> <br />
            <%= f.text_field :title %> <br />
        </p>
        <p>
            <%= f.label :content %> <br />
            <%= f.text_field :content %> <br />
        </p>
        <p>
            <%= f.label :address %> <br />
            <%= f.text_field :address %> <br />
        </p>
        <p>
            <%= f.submit "edit post" %>
        </p>
    <% end %>
